{
  "name": "southernins/env-manager",
  "description": "Commands to help check/verify ENV files are managed properly",
  "license": "MIT",
  "keywords": ["laravel", "env"],
  "authors": [
    {
      "name": "Nathan Crews",
      "email": "ncrews@southerninsurance.net"
    }
  ],
    "require": {
        "illuminate/support": "10.* || 11.*",
        "illuminate/console": "10.* || 11.*",
        "aws/aws-sdk-php": "^3.36",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
  "autoload": {
    "psr-4": {
      "SouthernIns\\EnvManager\\": "src/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "SouthernIns\\EnvManager\\EnvManagerServiceProvider"
      ]
    }

  }

}
